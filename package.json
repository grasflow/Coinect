    "lint-staged": "15.5.0",
    "msw": "^2.11.5",
    "prettier-plugin-astro": "0.14.1",
    "typescript-eslint": "8.28.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.1"
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
